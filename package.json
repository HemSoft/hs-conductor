{
  "name": "@hemsoft/conductor",
  "version": "0.1.0",
  "description": "Event-driven multi-agent orchestration with Inngest",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "conductor": "dist/index.js"
  },
  "scripts": {
    "build": "bun run tsc",
    "build:watch": "bun run tsc --watch",
    "dev": "bun run --watch src/index.ts",
    "start": "bun dist/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "keywords": [
    "inngest",
    "orchestration",
    "workflow",
    "event-driven",
    "multi-agent",
    "automation"
  ],
  "author": "HemSoft Developments",
  "license": "MIT",
  "dependencies": {
    "@github/copilot-sdk": "^0.1.9",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "cron-parser": "^5.5.0",
    "express": "^4.21.2",
    "fast-xml-parser": "^4.5.1",
    "inngest": "^3.31.0",
    "ora": "^8.1.1",
    "yaml": "^2.7.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/bun": "^1.3.7",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "bun@1.2.2"
}
